    def solve(self, R, C):
        
        max=0
        j=-1
        for i in range(C):
            if(self.get(0,i)==1):
                j=i
                break
        if(j==-1):
            j=C-1
        for i in range(R):
            while(j>=0 and self.get(i,j)==1):
                j-=1
                max=i
        return max

